elected-item`);\n        commit('selectItem', response.data.id);\n    },\n    async selectItem({ commit }, idData) {\n        await axios.post(`${backendBaseUrl}/api/items/select`, idData);\n        commit('selectItem', idData.id);\n    }\n};\n\nconst mutations = {\n    setItems: (state, items) => (state.items = items),\n    newItem: (state, item) => state.items.push(item),\n    removeItem: (state, id) =>\n        (state.items = state.items.filter(item => item.id !== id)),\n    selectItem: (state, id) => {\n        state.selected = id\n    },\n};\n\nexport default {\n    state,\n    getters,\n    actions,\n    mutations\n};\n"],"mappings":"AAAA,OAAOA,KAAK,MAAM,OAAO;AAEzB,MAAMC,KAAK,GAAG;EACVC,KAAK,EAAE,EAAE;EACTC,QAAQ,EAAE;AACd,CAAC;AAED,MAAMC,OAAO,GAAG,CAAC,CAAC;AAClB,MAAMC,cAAc,GAAG,uBAAuB;AAE9C,MAAMC,OAAO,GAAG;EACZ,MAAMC,WAAWA,CAAC;IAAEC;EAAO,CAAC,EAAE;IAC1B,MAAMC,QAAQ,GAAG,MAAMT,KAAK,CAACU,GAAG,CAAE,GAAEL,cAAe,YAAW,CAAC;IAC/DG,MAAM,CAAC,UAAU,EAAEC,QAAQ,CAACE,IAAI,CAAC;EACrC,CAAC;EACD,MAAMC,UAAUA,CAAC;IAAEJ;EAAO,CAAC,EAAEK,QAAQ,EAAE;IACnC,MAAMJ,QAAQ,GAAG,MAAMT,KAAK,CAACc,IAAI,CAAE,GAAET,cAAe,YAAW,EAAEQ,QAAQ,CAAC;IAC1EL,MAAM,CAAC,SAAS,EAAEC,QAAQ,CAACE,IAAI,CAAC;EACpC,CAAC;EACD,MAAMI,UAAUA,CAAC;IAAEP;EAAO,CAAC,EAAEQ,EAAE,EAAE;IAC7B,MAAMhB,KAAK,CAACiB,MAAM,CAAE,GAAEZ,cAAe,cAAaW,EAAG,EAAC,CAAC;IACvDR,MAAM,CAAC,YAAY,EAAEQ,EAAE,CAAC;EAC5B,CAAC;EACD,MAAME,eAAeA,CAAC;IAAEV;EAAO,CAAC,EAAE;IAC9B,MAAMC,QAAQ,GAAG,MAAMT,KAAK,CAACU,GAAG,CAAE,GAAEL,cAAe,8BAA6B,CAAC;IACjFG,MAAM,CAAC,YAAY,EAAEC,QAAQ,CAACE,IAAI,CAACK,EAAE,CAAC;EAC1C,CAAC;EACD,MAAMG,UAAUA,CAAC;IAAEX;EAAO,CAAC,EAAEY,MAAM,EAAE;IACjC,MAAMpB,KAAK,CAACc,IAAI,CAAE,GAAET,cAAe,mBAAkB,EAAEe,MAAM,CAAC;IAC9DZ,MAAM,CAAC,YAAY,EAAEY,MAAM,CAACJ,EAAE,CAAC;EACnC;AACJ,CAAC;AAED,MAAMK,SAAS,GAAG;EACdC,QAAQ,EAAEA,CAACrB,KAAK,EAAEC,KAAK,KAAMD,KAAK,CAACC,KAAK,GAAGA,KAAM;EACjDqB,OAAO,EAAEA,CAACtB,KAAK,EAAEuB,IAAI,KAAKvB,KAAK,CAACC,KAAK,CAACuB,IAAI,CAACD,IAAI,CAAC;EAChDT,UAAU,EAAEA,CAACd,KAAK,EAAEe,EAAE,KACjBf,KAAK,CAACC,KAAK,GAAGD,KAAK,CA