utton>\n          <button class=\"btn btn-danger\" @click=\"removeItem(item.id)\">\n            Remove\n          </button>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nexport default {\n  methods: {\n    selectItem(id) {\n      this.$store.dispatch('selectItem', {id: id});\n    },\n    removeItem(id) {\n      if (confirm('Are you sure?')) {\n        this.$store.dispatch('removeItem', id);\n      }\n    }\n  },\n  mounted() {\n    this.$store.dispatch('getAllItems');\n    this.$store.dispatch('getSelectedItem');\n  },\n  computed: {\n    items() {\n      return this.$store.state.items.items;\n    },\n    selectedItem() {\n      return this.$store.state.items.selected;\n    }\n  },\n};\n</script>"],"mappings":"AAoCA;EACAA,OAAA;IACAC,WAAAC,EAAA;MACA,KAAAC,MAAA,CAAAC,QAAA;QAAAF,EAAA,EAAAA;MAAA;IACA;IACAG,WAAAH,EAAA;MACA,IAAAI,OAAA;QACA,KAAAH,MAAA,CAAAC,QAAA,eAAAF,EAAA;MACA;IACA;EACA;EACAK,QAAA;IACA,KAAAJ,MAAA,CAAAC,QAAA;IACA,KAAAD,MAAA,CAAAC,QAAA;EACA;EACAI,QAAA;IACAC,MAAA;MACA,YAAAN,MAAA,CAAAO,KAAA,CAAAD,KAAA,CAAAA,KAAA;IACA;IACAE,aAAA;MACA,YAAAR,MAAA,CAAAO,KAAA,CAAAD,KAAA,CAAAG,QAAA;IACA;EACA;AACA"}]}